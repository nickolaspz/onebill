{
    "name": "ql/uri-template",
    "description": "An implementation of RFC 6570 level 4",
    "keywords": ["uri", "template", "url", "rfc6570", "uri template"],
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "phpunit/php-invoker": "*",
        "satooshi/php-coveralls": "dev-master@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Nagi",
            "email": "mattnagi@quickenloans.com"
        }
    ],
    "homepage": "https://github.com/QuickenLoans/uri-template",
    "support": {
        "email": "mattnagi@quickenloans.com",
        "issues": "https://github.com/QuickenLoans/uri-template/issues",
        "source": "https://github.com/QuickenLoans/uri-template"
    },
    "require": {
        "php": ">=5.5.0",
        "ext-ctype": "*",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "QL\\UriTemplate\\": "src"
        }
    },
    "archive": {
        "exclude": ["/bin", "/tests", "/phpunit.xml.dist"]
    }
}
